modules = ["python-3.11", "python3"]

[nix]
channel = "stable-24_05"

[workflows]

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Telegram Bot"

[[workflows.workflow]]
name = "Telegram Bot"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python telegram_bot.py"

[deployment]
run = ["sh", "-c", "python telegram_bot.py"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 5000
externalPort = 80
